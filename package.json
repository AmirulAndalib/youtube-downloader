{
  "name": "youtube-downloader",
  "version": "0.0.1",
  "description": "A browser extension for downloading almost any video from YouTube, right from its page.",
  "repository": "https://github.com/avi12/youtube-downloader",
  "author": "avi12 <avi6106@gmail.com>",
  "license": "MIT",
  "homepage": "https://avi12.com",
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "pack": "web-ext build --overwrite-dest --artifacts-dir dist_packed --source-dir dist --filename=YouTube-Downloader.zip",
    "build-pack": "pnpm build && pnpm run pack",
    "run-browser": "web-ext run --no-config-discovery --start-url https://www.youtube.com/playlist?list=PLwCbIpH3KfpCYpPglyn4T8ecOcSNuAkBr --source-dir dist --verbose --target",
    "run-chromium": "pnpm run-browser chromium",
    "run-firefox": "pnpm run-browser firefox-desktop"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.3",
    "@rollup/plugin-typescript": "^8.2.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/chrome": "^0.0.148",
    "@types/file-saver": "^2.0.3",
    "@types/jquery": "^3.5.6",
    "@types/youtube": "^0.0.44",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.53.3",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "2.6.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.36.0",
    "sass-lint": "^1.13.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "svelte": "^3.40.2",
    "svelte-materialify": "^0.3.11",
    "svelte-preprocess": "^4.7.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "web-ext": "^6.2.0"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "0.10.0",
    "blob-util": "^2.0.2",
    "browser-id3-writer": "^4.4.0",
    "file-saver": "^2.0.5",
    "get-artist-title": "^1.3.1",
    "vue": "^2.6.14"
  }
}
