    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.45.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.37.0",
    "svelte-check": "^1.5.1",
    "svelte-materialify": "^0.3.10",
    "svelte-preprocess": "^4.7.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.9.7"
  },
  "prettier": {
    "arrowParens": "avoid",
